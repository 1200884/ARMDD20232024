IF NOT EXISTS(SELECT name FROM sys.tables WHERE name = 'GenderDescription_Lookup')
BEGIN
	CREATE TABLE [dbo].[GenderDescription_Lookup](
		[Gender] [char](1) NULL,
		[GenderDescription] [nvarchar](10) NOT NULL
	) ON [PRIMARY]
	INSERT [dbo].[GenderDescription_Lookup] ([Gender], [GenderDescription]) VALUES (NULL, N'Unknown')
	INSERT [dbo].[GenderDescription_Lookup] ([Gender], [GenderDescription]) VALUES (N'F', N'Female')
	INSERT [dbo].[GenderDescription_Lookup] ([Gender], [GenderDescription]) VALUES (N'M', N'Male')
END